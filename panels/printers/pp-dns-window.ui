<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="PpDnsWindow" parent="GtkWindow">
    <property name="width-request">200</property>
    <property name="height-request">300</property>
    <property name="title" translatable="1">DNS-SD services</property>
    <property name="child">
      <object class="GtkBox" id="dw_top_box_in">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox" id="dw_main">
            <property name="vexpand">1</property>
            <property name="margin-start">30</property>
            <property name="margin-end">30</property>
            <property name="margin-top">10</property>
            <property name="margin-bottom">10</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkListBox" id="dw_right_list">
                <property name="vexpand">1</property>
                <property name="margin-start">10</property>
                <property name="margin-end">10</property>
                <property name="margin-top">10</property>
                <property name="margin-bottom">10</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
